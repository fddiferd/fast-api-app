# Example template - Copy this to terraform.tfvars (which is gitignored)
# This file will be automatically generated by bootstrap.sh
environment = "dev"  # Will be set based on MODE in .env
billing_account_id = ""  # Will be set from GCP_BILLING_ACCOUNT_ID in .env
project_ids = {
  dev = "template-backend-dev-yourusername"  # Will be dynamically generated
}
region = "us-central1"  # From config file
service_name = "backend-rest-api"  # From config file
repo_name = "template-backend"  # From config file 